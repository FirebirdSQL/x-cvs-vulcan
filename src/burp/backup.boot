	stuff(&blr, blr_rse);
	stuff(&blr, 1);					// count of relations 
	stuff(&blr, blr_rid);
	stuff_word(&blr, relation->rel_id);
	stuff(&blr, 0);					// context variable 
	stuff(&blr, blr_end);

	stuff(&blr, blr_send);
	stuff(&blr, 0);
	stuff(&blr, blr_begin);
	stuff(&blr, blr_assignment);
	stuff(&blr, blr_literal);
	stuff(&blr, blr_short);
	stuff(&blr, 0);
	stuff_word(&blr, 1);
	stuff(&blr, blr_parameter);
	stuff(&blr, 0);
	stuff_word(&blr, eof_parameter);

