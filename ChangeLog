2006-06-05 14:48  verbguy

	* vulcan/src/dsql/ddl.cpp:

	first pass at back-porting dmitry's fix for cf_isql_31 from fb2 to
	vulcan. This involves a trigger and the COALESCE function. Full
	test case is:

	CREATE DATABASE 'test.fdb' USER 'SYSDBA' PASSWORD 'masterkey';

	set echo;

	CREATE DOMAIN DM_ID AS BIGINT  NOT NULL;

	CREATE TABLE ITEMS (   UID DM_ID PRIMARY KEY,	DESCRIPTION
	VARCHAR(80));

	CREATE TABLE MITEMS (	UID DM_ID PRIMARY KEY,	 ITEM_UID DM_ID
	REFERENCES ITEMS(UID) ON DELETE CASCADE,   MFIELD4 VARCHAR(15)
	DEFAULT '' NOT NULL);

	CREATE TABLE MITEMS_EXPIRY (   UID DM_ID PRIMARY KEY,	EXPIRY_DATE
	DATE);

	CREATE TABLE MITEMS_EXPIRY_WAREHOUSES (   QOH integer);

	ALTER TABLE MITEMS_EXPIRY   ADD QOH COMPUTED BY (COALESCE((SELECT
	SUM(QOH) FROM MITEMS_EXPIRY_WAREHOUSES),0));

	ALTER TABLE MITEMS   ADD QOH COMPUTED BY (COALESCE((SELECT SUM(QOH)
	FROM MITEMS_EXPIRY),0));

	ALTER TABLE ITEMS   ADD QOH COMPUTED BY (COALESCE((SELECT SUM(QOH)
	FROM MITEMS),0));

	SET TERM ^;

	CREATE GENERATOR ITEMS_GEN^

	CREATE TRIGGER ITEMS_TR0 FOR ITEMS  ACTIVE BEFORE INSERT POSITION 0
	AS   BEGIN    IF (NEW.UID IS NULL OR NEW.UID<0) THEN
	NEW.UID=GEN_ID(ITEMS_GEN,1);   END^

	CREATE GENERATOR MITEMS_GEN^

	CREATE TRIGGER ITEMS_TR2 FOR ITEMS  ACTIVE AFTER INSERT POSITION 2
	AS   BEGIN     INSERT INTO MITEMS(UID, ITEM_UID)
	VALUES(GEN_ID(MITEMS_GEN,1), NEW.UID);	 END^

	SET TERM ;^

	commit;

	INSERT INTO ITEMS(DESCRIPTION) VALUES('PA');

	commit;

	SHOW TABLE ITEMS;

	SELECT	 QOH, QOH, QOH, QOH, QOH, QOH, QOH, QOH, QOH, QOH FROM
	ITEMS;

	DROP DATABASE;

